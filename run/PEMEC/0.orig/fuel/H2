FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          yH2;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.5857;

boundaryField
{
    fuelInlet
    {
        type            fixedValue;
        value           $internalField;
    }

    fuelOutlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    fuelSides
    {
        type            zeroGradient;
    }

    fuelInletSides
    {
        type            zeroGradient;
    }

    fuelOutletSides
    {
        type            zeroGradient;
    }
    
    anodeSides
    {
        type            zeroGradient;
    }
    
    aflSides
    {
        type            zeroGradient;
    }
    
    fuel_to_electrolyte //anode
    {
        type            fixedGradient;
        gradient        uniform 0;
    }

    fuel_to_interconnect
    {
        type            zeroGradient;
    }
}


