FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/air";
    object      alpha.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    airInlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    airOutlet
    {
        type            inletOutlet;
        phi             phi.air;
        inletValue      uniform 0;
        value           uniform 0;
    }
    cathodeSides
    {
        type            zeroGradient;
    }
    air_to_interconnect
    {
        type            zeroGradient;
    }
    air_to_electrolyte
    {
        type            zeroGradient;
    }
}


