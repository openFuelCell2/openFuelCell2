FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          phi;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 1.2; // uniform (0.566711 0 0);

boundaryField
{

    interconnect0
    {
        type            fixedValue;
        value           $internalField;
    }

	phiEC_to_phiI0
    {
        type            zeroGradient;
    }
    
    phiEC_to_phi0
    {
        type            zeroGradient;
    }
    
    interconnectSides
    {
        type            zeroGradient;
    }
    
	cathodeSides
    {
        type            zeroGradient;
    }
    
	anodeSides
    {
        type            zeroGradient;
    }
    
	phiE_to_phi0
    {
        type            zeroGradient;
    }
}


