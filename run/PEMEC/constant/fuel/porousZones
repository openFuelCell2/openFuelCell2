FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          porousZones;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

anode
{
    type            DarcyForchheimer;	// porosity Model: fixCoeff, powerLaw, DarcyForchheimer
    active          yes;		// status
    cellZone        anode;		// name of porous zone
    porosity        0.7;		// porosity

    DarcyForchheimerCoeffs		// coefficents for DarcyForchheimer; with the name of type + Coeffs
    {
        d   (1e11 1e11 1e11);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);
            rotation
            {
                type    axesRotation;
                e1      (1 0 0);    //(0.70710678 0.70710678 0);
                e2      (0 0 1);
            }
        }
    }

    Cp          568;
    k           1.7;
}

mpl
{
    type            DarcyForchheimer;	// porosity Model: fixCoeff, powerLaw, DarcyForchheimer
    active          yes;		// status
    cellZone        mpl;		// name of porous zone
    porosity        0.4;		// porosity

    DarcyForchheimerCoeffs		// coefficents for DarcyForchheimer; with the name of type + Coeffs
    {
        d   (1e12 1e12 1e12);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);
            rotation
            {
                type    axesRotation;
                e1      (1 0 0);    //(0.70710678 0.70710678 0);
                e2      (0 0 1);
            }
        }
    }

    Cp  3300;
    k   1;
}

acl
{
    type            DarcyForchheimer;	// porosity Model: fixCoeff, powerLaw, DarcyForchheimer
    active          yes;		// status
    cellZone        acl;		// name of porous zone
    porosity        0.3;		// porosity

    DarcyForchheimerCoeffs		// coefficents for DarcyForchheimer; with the name of type + Coeffs
    {
        d   (1e13 1e13 1e13);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);
            rotation
            {
                type    axesRotation;
                e1      (1 0 0);    //(0.70710678 0.70710678 0);
                e2      (0 0 1);
            }
        }
    }

    Cp  3300;
    k   1;
}
