//- Add information here

control           true;
relax             1.0e-5;

galvanostatic
{
    active      true;
    region      phiEC;
    patchName   interconnect0;

    ibar -8000;
}


sigma
{
    bpp
    {
        sigmaModel      constantSigma;

        constantSigmaCoeffs
        {
            cellZone        bpp;
            sigma           20000;
        }
    }

    ccl
    {
        sigmaModel      porousSigma;

        porousSigmaCoeffs
        {
            cellZone        ccl;
            catalyst        no;
            porosity        0.3;
            sigma           5000;
        }
    }

    cmpl
    {
        sigmaModel      porousSigma;

        porousSigmaCoeffs
        {
            cellZone        cmpl;
            catalyst        no;
            porosity        0.4;
            sigma           8000;
        }
    }

    cathode
    {
        sigmaModel      porousSigma;

        porousSigmaCoeffs
        {
            cellZone        cathode;
            catalyst        no;
            porosity        0.7;
            sigma           8000;
        }
    }
}
