FoamFile
{
    version         2.0;
    format          ascii;
    class           volVectorField;
    object          U;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 1 -1 0 0 0 0];

internalField    uniform (0. 0 0);  // uniform (0.209933 0 0);

boundaryField
{
    ".*"
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    hotInlet
    {
        type            fixedValue;
    		value		        uniform (2.36 0 0);
    }

    hotOutlet
    {
        type           inletOutlet;
        phi            phi;
        value          $internalField;
        inletValue     uniform (0 0 0);
    }
}


