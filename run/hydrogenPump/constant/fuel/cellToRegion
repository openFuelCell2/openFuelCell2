FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1/fuel";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    anodeSides
    {
        type            zeroGradient;
    }
    fuelInlet
    {
        type            zeroGradient;
    }
    fuelOutlet
    {
        type            zeroGradient;
    }
    fuel_to_electrolyte
    {
        type            calculated;
        value           uniform 0;
    }
    fuel_to_interconnect
    {
        type            calculated;
        value           uniform 0;
    }
}


