FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      combustionProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

combustionModel electroChemicalReaction;

dissolved       false;

saturation
{
    type ArdenBuck;
}

activationOverpotentialModel
{
    type    ButlerVolmer;

    zoneName  ccl;

    phaseChange     false;

    regions
    {
        fluid
        {
            name    air;
        }
        electron
        {
            name    phiEC;
        }
        ion
        {
            name    phiI;
        }
        dissolved
        {
            name    dissolved;
        }
    }

    species
    {
        O2
        {
            ksi     1.0;
            cRef    5.4;
        }
    }

    phiNames
    (
        electron    phi
        ion         phi
    );

    j0              1.0e8;
    relax           0.3;
    alpha           0.5;
    gamma           1.5;

    nernstModel
    {
        type        standard;

        pRef        101325;

        RxnList
        (
            O2      0.5
            e       2
        );

    }
}



