FoamFile
{
    version         2.0;
    format          ascii;
    class           volVectorField;
    object          U;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (4 0 0); // uniform (0.566711 0 0);

boundaryField
{

    airInlet
    {
        type            fixedValue;
		value		    uniform (4 0 0);
    }

    airOutlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           $internalField;
    }

    airSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }

    airInletSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }

    airOutletSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }

    cathodeSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }    
/*    cflSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }
*/        
    air_to_interconnect
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    air_to_electrolyte //cathode
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
}


